            function selectLocalImage(idEditor, editor) {
                
                const input = document.createElement('input');
                input.setAttribute('type', 'file');
                input.click();
                // Listen upload local image and save to server
                input.onchange = () => {
                    const file = input.files[0];
                    // file type is only image.
                    if (/^image\//.test(file.type)) {
                    const reader = new FileReader();
                    reader.readAsDataURL(file);
                    reader.onload = function () {
                    //console.log(reader.result);
                    var selection = editor.getSelection(true);
                    var imgname = "<a href='http://"+reader.result+"' target='1' onclick='test();'>"+file.name+"</a>";
                    //editor.insertText(selection.index, file.name);
                    editor.clipboard.dangerouslyPasteHTML(selection.index, imgname,'silent');
                    $('#'+idEditor).find( "a[href^='data:image']").unbind('hover');
                    $('#'+idEditor).find("a[href^='data:image']").hover(function (evt){
                    showImage($(this).attr('href'));
                    
                });
            };
            } else {
                console.warn('You could only upload images.');
            }
            };
