        <div class="popup js-popup popup-login" id="popup-add-activity">
            <div class="popup__background js-popup-close"></div><!-- /.popup__background -->
            
            <div class="popup__inner">
                <button type="button" class="js-popup-close popup__close">
                    <img src="assets/images/png/popup-close.png" alt="Close Icon"/>
                </button><!-- popup__close -->
                
                <div class="form form-steps">
                    <form action="?" method="post">
                        <div class="steps" id="steps">
                            
                            
                            <!-- STEP ONE -->
                            
                            
                            <div class="step active" data-step-index="0">
                                <div class="login">
                                    <div class="login__head">
                                        <h4 class="login__title">
                                            Activity Details
                                        </h4><!-- /.login__title -->
                                    </div><!-- /.login__head -->
                                    
                                    <div class="login__body">
                                        <div class="form-group">
                                            <label for="activity_name">Activity Name</label>
                                            
                                            <input type="text" name="activity-name" id="activity_name" class="form-control form-control--big" placeholder="Enter the planned activity name"/>
                                        </div><!-- /.form-group -->
                                        
                                        <div class="form-group">
                                            <label for="primaryHop">Primary HOP Outcome</label>
                                            
                                            <div class="select-big">
                                                <select name="primary-hop" id="primaryHop">
                                                    <option data-display="Select a HOP Outcome to assign this activity to"></option>
                                                    
                                                    <option value="1">Option 2</option>
                                                    
                                                    <option value="1">Option 3</option>
                                                    
                                                    <option value="1">Option 4</option>
                                                </select>
                                            </div><!-- /.select-big -->
                                        </div><!-- /.form-group -->
                                    </div><!-- /.login__body -->
                                </div><!-- /.login -->
                            </div><!-- /.step -->
                            
                            
                            <!-- STEP Two -->
                            
                            
                            <div class="step" data-step-index="1">
                                <div class="login">
                                    <div class="login__head">
                                        <h4 class="login__title">
                                            Purpose of Activity
                                        </h4><!-- /.login__title -->
                                    </div><!-- /.login__head -->
                                    
                                    <div class="login__body">
                                        <div class="form-group">
                                            <label for="activityPurpose">What is the purpose of this activity?</label>
                                            
                                            <textarea name="activity-purpose" id="activityPurpose" class="form-control form-textarea" placeholder="A breif description outlining the purpose of this activity"></textarea>
                                        </div><!-- /.form-group -->
                                    </div><!-- /.login__body -->
                                </div><!-- /.login -->
                            </div><!-- /.step -->
                            
                            
                            <!-- STEP Three -->
                            
                            
                            <div class="step" data-step-index="2">
                                <div class="login">
                                    <div class="login__head">
                                        <h4 class="login__title">
                                            Expectations of Impact
                                        </h4><!-- /.login__title -->
                                    </div><!-- /.login__head -->
                                    
                                    <div class="login__body">
                                        <div class="form-group">
                                            <label for="impact">What impact will this have on whanau hapu iwi?</label>
                                            
                                            <textarea name="what-impact" id="impact" class="form-control form-textarea" placeholder="Breifly describe the impact this activity will have on whanau, hapu and iwi."></textarea>
                                        </div><!-- /.form-group -->
                                    </div><!-- /.login__body -->
                                </div><!-- /.login -->
                            </div><!-- /.step -->
                            
                            
                            <div class="steps__actions">
                                <button type="button" class="js-move step__btn step__prev" data-move-to="prev">Go Back</button>
                                
                                <div class="steps__dots">
                                    <ul>
                                        <li class="active">
                                            <button type="button" class="js-move step__dot" data-move-to="0">Dot</button>
                                        </li>
                                        
                                        <li>
                                            <button type="button" class="js-move step__dot" data-move-to="1">Dot</button>
                                        </li>
                                        
                                        <li>
                                            <button type="button" class="js-move step__dot" data-move-to="2">Dot</button>
                                        </li>
                                    </ul>
                                    
                                    
                                </div><!-- /.steps__dots -->
                                
                                <button type="button" class="js-move step__btn step__next" data-move-to="next">Next</button>
                                
                                <button type="submit" value="GO" class="js-popup-close step__btn step__submit">SAVE</button> 
                            </div><!-- /.steps__actions -->
                        </div><!-- /.steps -->
                    </form>
                </div>
            </div><!-- /.form -->
        </div><!-- /.popup__inner -->

    
    <!--Use aura:if tag to display/hide popup based on isModalOpen value-->  
    
    <aura:if isTrue="{!v.isModalOpen}">             
        <c:createActivitiy parent="{! this}"/>
    </aura:if>
    <aura:if isTrue="{!v.isDeletePopup}">
        
        <!-- Modal/Popup Box starts here-->
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container">
                <!-- Modal/Popup Box Header Starts here-->
                <header class="slds-modal__header">
                    <lightning:buttonIcon iconName="utility:close"
                                          onclick="{!c.closeDeleteModal }"
                                          alternativeText="close"
                                          variant="bare-inverse"
                                          class="slds-modal__close"/>
                    <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Are you sure?</h2>
                </header>
                <!--Modal/Popup Box Body Starts here-->
                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                    <p style="text-align:center">If you delete this activity you won't be able to restore this activity.
                        
                    </p>
                </div>
                <!--Modal/Popup Box Footer Starts here-->
                <footer class="slds-modal__footer">
                    <lightning:button variant="neutral"
                                      label="No"
                                      title="No"
                                      onclick="{! c.closeDeleteModal }"/>
                    <lightning:button variant="brand"
                                      label="Yes,I want to delete"
                                      title="Yes"
                                      onclick="{!c.deleteDetails}"
                                      />
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
</aura:component>