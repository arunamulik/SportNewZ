<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Non_Admin_Cannot_Change_Name_Field</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISCHANGED(Name), $Profile.Name&lt;&gt;&apos;System Administrator&apos;)</errorConditionFormula>
    <errorMessage>Sorry you don&apos;t have privilege to change the name of investment schedules.</errorMessage>
</ValidationRule>
